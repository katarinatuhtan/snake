<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#001800</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#4FA64F</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#001800</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#EBEBEB</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Motion Control Neue Lite</FontFamily>
    </Application.Resources>
</Application>
